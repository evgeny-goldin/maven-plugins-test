<?xml version="1.0" encoding="UTF-8"?>

<project xmlns              = "http://maven.apache.org/POM/4.0.0"
         xmlns:xsi          = "http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation = "http://maven.apache.org/POM/4.0.0
                               http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>
    <groupId>com.github.goldin.maven-plugins-test</groupId>
    <artifactId>copy-maven-plugin.issues.0.2.4</artifactId>
    <packaging>jar</packaging>
    <version>0.1-SNAPSHOT</version>
    <name>[${project.groupId}:${project.artifactId}:${project.version}]</name>


    <parent>
        <groupId>com.github.goldin.maven-plugins-test</groupId>
        <artifactId>copy-maven-plugin.issues</artifactId>
        <version>0.1-SNAPSHOT</version>
    </parent>


    <dependencies>
        <dependency>
            <groupId>org.springframework</groupId>
            <artifactId>spring-context</artifactId>
            <version>${spring-version}</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>${junit-version}</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>org.codehaus.jsr166-mirror</groupId>
            <artifactId>jsr166</artifactId>
            <version>1.7.0</version>
            <scope>provided</scope>
        </dependency>
    </dependencies>


    <build>
        <plugins>

            <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
            <!-- Copying Spring transitive dependencies using standard dependency plugin -->
            <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <version>2.4</version>
                <executions>
                    <execution>
                        <id>copy-spring-transitive-dependencies</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>copy-dependencies</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${outputDir}/pl-550/dependency-plugin</outputDirectory>
                            <excludeTransitive>false</excludeTransitive>
                        </configuration>
                    </execution>
                </executions>
            </plugin>


            <plugin>
                <groupId>com.github.goldin</groupId>
                <artifactId>copy-maven-plugin</artifactId>
                <version>${plugins-version}</version>
                <executions>
                    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                    <!-- http://evgeny-goldin.org/youtrack/issue/pl-617                         -->
                    <!-- Allow to set compression level when Zip archives are created using Ant -->
                    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                    <execution>
                        <id>http://evgeny-goldin.org/youtrack/issue/pl-617</id>
                        <goals>
                            <goal>copy</goal>
                        </goals>
                        <phase>initialize</phase>
                        <configuration>
                            <resources>
                                <resource>
                                    <targetPath>${outputDir}/pl-617/unpack</targetPath>
                                    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                                    <!-- Unpacking some heavy dependencies to pack them later with varying compression level -->
                                    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                                    <dependencies>
                                        <dependency>
                                            <groupId>org.artifactory</groupId>
                                            <artifactId>artifactory-web-war</artifactId>
                                            <version>${artifactory-version}</version>
                                            <type>war</type>
                                        </dependency>
                                        <dependency>
                                            <groupId>org.jenkins-ci.main</groupId>
                                            <artifactId>jenkins-war</artifactId>
                                            <version>${jenkins-version}</version>
                                            <type>war</type>
                                        </dependency>
                                        <dependency>
                                            <groupId>org.apache.maven</groupId>
                                            <artifactId>apache-maven</artifactId>
                                            <version>${maven-version}</version>
                                            <classifier>bin</classifier>
                                            <type>tar.gz</type>
                                        </dependency>
                                        <dependency>
                                            <groupId>org.apache.maven</groupId>
                                            <artifactId>apache-maven</artifactId>
                                            <version>${maven-version}</version>
                                            <classifier>src</classifier>
                                            <type>tar.gz</type>
                                        </dependency>
                                    </dependencies>
                                    <unpack>true</unpack>
                                </resource>
                                <resource>
                                    <targetPath>${outputDir}/pl-617/1-Ant.tar</targetPath>
                                    <directory>${outputDir}/pl-617/unpack</directory>
                                    <pack>true</pack>
                                </resource>
                                <resource>
                                    <targetPath>${outputDir}/pl-617/2-Ant.zip</targetPath>
                                    <directory>${outputDir}/pl-617/unpack</directory>
                                    <pack>true</pack>
                                    <compressionLevel>0</compressionLevel>
                                </resource>
                                <resource>
                                    <targetPath>${outputDir}/pl-617/3-Ant.zip</targetPath>
                                    <directory>${outputDir}/pl-617/unpack</directory>
                                    <pack>true</pack>
                                    <compressionLevel>5</compressionLevel>
                                </resource>
                                <resource>
                                    <targetPath>${outputDir}/pl-617/4-Ant.zip</targetPath>
                                    <directory>${outputDir}/pl-617/unpack</directory>
                                    <pack>true</pack>
                                    <compressionLevel>9</compressionLevel>
                                </resource>
                                <resource>
                                    <targetPath>${outputDir}/pl-617/5-Ant.tar.gz</targetPath>
                                    <directory>${outputDir}/pl-617/unpack</directory>
                                    <pack>true</pack>
                                </resource>
                                <resource>
                                    <targetPath>${outputDir}/pl-617/1-TrueZip.tar</targetPath>
                                    <directory>${outputDir}/pl-617/unpack</directory>
                                    <useTrueZipForPack>true</useTrueZipForPack>
                                    <pack>true</pack>
                                </resource>
                                <resource>
                                    <targetPath>${outputDir}/pl-617/2-TrueZip.zip</targetPath>
                                    <directory>${outputDir}/pl-617/unpack</directory>
                                    <pack>true</pack>
                                    <useTrueZipForPack>true</useTrueZipForPack>
                                </resource>
                                <resource>
                                    <targetPath>${outputDir}/pl-617/3-TrueZip.tar.gz</targetPath>
                                    <directory>${outputDir}/pl-617/unpack</directory>
                                    <useTrueZipForPack>true</useTrueZipForPack>
                                    <pack>true</pack>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                    <!-- http://evgeny-goldin.org/youtrack/issue/pl-550                                       -->
                    <!-- Make both <includeScope> and <excludeScope> work together for filtering dependencies -->
                    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                    <execution>
                        <id>http://evgeny-goldin.org/youtrack/issue/pl-550</id>
                        <goals>
                            <goal>copy</goal>
                        </goals>
                        <phase>initialize</phase>
                        <configuration>
                            <resources>
                                <!-- All transitive dependencies from all scopes -->
                                <resource>
                                    <description>all</description>
                                    <targetPath>${outputDir}/pl-550/all</targetPath>
                                    <dependency>
                                        <groupId>org.springframework</groupId>
                                        <artifactId>spring-context</artifactId>
                                        <version>${spring-version}</version>
                                        <excludeTransitive>false</excludeTransitive>
                                        <!-- Not available in Maven repos -->
                                        <excludeGroupIds>com.sun.jmx,com.sun.jdmk</excludeGroupIds>
                                    </dependency>
                                </resource>
                                <!-- All transitive dependencies from all scopes, including optional -->
                                <resource>
                                    <description>optional-1</description>
                                    <targetPath>${outputDir}/pl-550/optional-1</targetPath>
                                    <dependency>
                                        <groupId>org.springframework</groupId>
                                        <artifactId>spring-context</artifactId>
                                        <version>${spring-version}</version>
                                        <excludeTransitive>false</excludeTransitive>
                                        <includeOptional>true</includeOptional>
                                        <!-- Not available in Maven repos -->
                                        <excludeGroupIds>com.sun.jmx,com.sun.jdmk,com.sun,javax.security,javax.jws,javax.mail,javax.activation</excludeGroupIds>
                                        <excludeArtifactIds>xerces-impl,xml-apis,commons-logging,commons-logging-api,stax-ri,hsqldb</excludeArtifactIds>
                                    </dependency>
                                </resource>
                                <!-- All non-transitive dependencies from all scopes, including optional -->
                                <resource>
                                    <description>optional-2</description>
                                    <targetPath>${outputDir}/pl-550/optional-2</targetPath>
                                    <dependency>
                                        <groupId>org.springframework</groupId>
                                        <artifactId>spring-context</artifactId>
                                        <version>${spring-version}</version>
                                        <includeOptional>true</includeOptional>
                                    </dependency>
                                </resource>
                                <!-- Single dependency, no transitivity -->
                                <resource>
                                    <description>single</description>
                                    <targetPath>${outputDir}/pl-550/single</targetPath>
                                    <dependency>
                                        <groupId>org.springframework</groupId>
                                        <artifactId>spring-context</artifactId>
                                        <version>${spring-version}</version>
                                    </dependency>
                                </resource>
                                <!-- Single "filtering" dependency from "compile" scope, no transitivity -->
                                <resource>
                                    <description>compile</description>
                                    <targetPath>${outputDir}/pl-550/compile</targetPath>
                                    <dependency>
                                        <includeScope>compile</includeScope>
                                        <excludeTransitive>true</excludeTransitive>
                                    </dependency>
                                </resource>
                                <!-- Single "filtering" dependency from "test" scope, no transitivity -->
                                <resource>
                                    <description>test</description>
                                    <targetPath>${outputDir}/pl-550/test</targetPath>
                                    <dependency>
                                        <includeScope>test</includeScope>
                                        <excludeTransitive>true</excludeTransitive>
                                    </dependency>
                                </resource>
                                <!-- Single "filtering" dependency from "provided" scope, no transitivity -->
                                <resource>
                                    <description>provided</description>
                                    <targetPath>${outputDir}/pl-550/provided</targetPath>
                                    <dependency>
                                        <includeScope>provided</includeScope>
                                        <excludeTransitive>true</excludeTransitive>
                                    </dependency>
                                </resource>
                                <!-- Transitive dependencies from "compile" scope -->
                                <resource>
                                    <description>1</description>
                                    <targetPath>${outputDir}/pl-550/1</targetPath>
                                    <dependency>
                                        <groupId>org.springframework</groupId>
                                        <artifactId>spring-context</artifactId>
                                        <version>${spring-version}</version>
                                        <excludeTransitive>false</excludeTransitive>
                                        <includeScope>compile</includeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive dependencies from "test" scope -->
                                <resource>
                                    <description>2</description>
                                    <targetPath>${outputDir}/pl-550/2</targetPath>
                                    <dependency>
                                        <groupId>org.springframework</groupId>
                                        <artifactId>spring-context</artifactId>
                                        <version>${spring-version}</version>
                                        <excludeTransitive>false</excludeTransitive>
                                        <includeScope>test</includeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive dependencies from "provided" scope -->
                                <resource>
                                    <description>3</description>
                                    <targetPath>${outputDir}/pl-550/3</targetPath>
                                    <dependency>
                                        <groupId>org.springframework</groupId>
                                        <artifactId>spring-context</artifactId>
                                        <version>${spring-version}</version>
                                        <excludeTransitive>false</excludeTransitive>
                                        <includeScope>provided</includeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive dependencies from "compile" scope, "test" and "provided" excluded -->
                                <resource>
                                    <description>4</description>
                                    <targetPath>${outputDir}/pl-550/4</targetPath>
                                    <dependency>
                                        <groupId>org.springframework</groupId>
                                        <artifactId>spring-context</artifactId>
                                        <version>${spring-version}</version>
                                        <excludeTransitive>false</excludeTransitive>
                                        <includeScope>compile</includeScope>
                                        <excludeScope>test, provided</excludeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive dependencies from "compile" scope, "test" excluded -->
                                <resource>
                                    <description>5</description>
                                    <targetPath>${outputDir}/pl-550/5</targetPath>
                                    <dependency>
                                        <groupId>org.springframework</groupId>
                                        <artifactId>spring-context</artifactId>
                                        <version>${spring-version}</version>
                                        <excludeTransitive>false</excludeTransitive>
                                        <includeScope>compile</includeScope>
                                        <excludeScope>test</excludeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive dependencies from "compile" scope, "provided" excluded -->
                                <resource>
                                    <description>6</description>
                                    <targetPath>${outputDir}/pl-550/6</targetPath>
                                    <dependency>
                                        <groupId>org.springframework</groupId>
                                        <artifactId>spring-context</artifactId>
                                        <version>${spring-version}</version>
                                        <excludeTransitive>false</excludeTransitive>
                                        <includeScope>compile</includeScope>
                                        <excludeScope>provided</excludeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive dependencies from "test" scope, "compile" excluded -->
                                <resource>
                                    <description>7</description>
                                    <targetPath>${outputDir}/pl-550/7</targetPath>
                                    <dependency>
                                        <groupId>org.springframework</groupId>
                                        <artifactId>spring-context</artifactId>
                                        <version>${spring-version}</version>
                                        <excludeTransitive>false</excludeTransitive>
                                        <includeScope>test</includeScope>
                                        <excludeScope>compile</excludeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive dependencies from "provided" scope, "compile" excluded -->
                                <resource>
                                    <description>8</description>
                                    <targetPath>${outputDir}/pl-550/8</targetPath>
                                    <dependency>
                                        <groupId>org.springframework</groupId>
                                        <artifactId>spring-context</artifactId>
                                        <version>${spring-version}</version>
                                        <excludeTransitive>false</excludeTransitive>
                                        <includeScope>provided</includeScope>
                                        <excludeScope>compile</excludeScope>
                                    </dependency>
                                </resource>
                                <!-- All transitive dependencies, "test" excluded -->
                                <resource>
                                    <description>9</description>
                                    <targetPath>${outputDir}/pl-550/9</targetPath>
                                    <dependency>
                                        <groupId>org.springframework</groupId>
                                        <artifactId>spring-context</artifactId>
                                        <version>${spring-version}</version>
                                        <excludeTransitive>false</excludeTransitive>
                                        <excludeScope>test</excludeScope>
                                    </dependency>
                                </resource>
                                <!-- All transitive dependencies, "provided" excluded -->
                                <resource>
                                    <description>10</description>
                                    <targetPath>${outputDir}/pl-550/10</targetPath>
                                    <dependency>
                                        <groupId>org.springframework</groupId>
                                        <artifactId>spring-context</artifactId>
                                        <version>${spring-version}</version>
                                        <excludeTransitive>false</excludeTransitive>
                                        <excludeScope>provided</excludeScope>
                                        <!-- Not available in Maven repos -->
                                        <excludeGroupIds>com.sun.jmx,com.sun.jdmk</excludeGroupIds>
                                    </dependency>
                                </resource>
                                <!-- Transitive "filtering" dependency with "compile" scope -->
                                <resource>
                                    <description>12</description>
                                    <targetPath>${outputDir}/pl-550/12</targetPath>
                                    <dependency>
                                        <includeScope>compile</includeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive "filtering" dependency with "test" scope -->
                                <resource>
                                    <description>22</description>
                                    <targetPath>${outputDir}/pl-550/22</targetPath>
                                    <dependency>
                                        <includeScope>test</includeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive "filtering" dependency with "provided" scope -->
                                <resource>
                                    <description>32</description>
                                    <targetPath>${outputDir}/pl-550/32</targetPath>
                                    <dependency>
                                        <includeScope>provided</includeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive "filtering" dependency with "compile" scope, "test" and "provided" excluded -->
                                <resource>
                                    <description>42</description>
                                    <targetPath>${outputDir}/pl-550/42</targetPath>
                                    <dependency>
                                        <includeScope>compile</includeScope>
                                        <excludeScope>test, provided</excludeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive "filtering" dependency with "compile" scope, "test" excluded -->
                                <resource>
                                    <description>52</description>
                                    <targetPath>${outputDir}/pl-550/52</targetPath>
                                    <dependency>
                                        <includeScope>compile</includeScope>
                                        <excludeScope>test</excludeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive "filtering" dependency with "compile" scope, "provided" excluded -->
                                <resource>
                                    <description>62</description>
                                    <targetPath>${outputDir}/pl-550/62</targetPath>
                                    <dependency>
                                        <includeScope>compile</includeScope>
                                        <excludeScope>provided</excludeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive "filtering" dependency with "test" scope, "compile" excluded -->
                                <resource>
                                    <description>72</description>
                                    <targetPath>${outputDir}/pl-550/72</targetPath>
                                    <dependency>
                                        <includeScope>test</includeScope>
                                        <excludeScope>compile</excludeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive "filtering" dependency with "provided" scope, "compile" excluded -->
                                <resource>
                                    <description>82</description>
                                    <targetPath>${outputDir}/pl-550/82</targetPath>
                                    <dependency>
                                        <includeScope>provided</includeScope>
                                        <excludeScope>compile</excludeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive "filtering" dependency, "test" scope excluded -->
                                <resource>
                                    <description>92</description>
                                    <targetPath>${outputDir}/pl-550/92</targetPath>
                                    <dependency>
                                        <excludeScope>test</excludeScope>
                                    </dependency>
                                </resource>
                                <!-- Transitive "filtering" dependency, "provided" scope excluded -->
                                <resource>
                                    <description>102</description>
                                    <targetPath>${outputDir}/pl-550/102</targetPath>
                                    <dependency>
                                        <excludeScope>provided</excludeScope>
                                    </dependency>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                </executions>
            </plugin>

            <plugin>
                <groupId>com.github.goldin</groupId>
                <artifactId>assert-maven-plugin</artifactId>
                <version>${plugins-version}</version>
                <executions>
                    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                    <!-- http://evgeny-goldin.org/youtrack/issue/pl-617 -->
                    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                    <execution>
                        <id>http://evgeny-goldin.org/youtrack/issue/pl-617</id>
                        <goals>
                            <goal>assert</goal>
                        </goals>
                        <phase>test</phase>
                        <configuration>
                            <assertFiles>
                                ${outputDir}/pl-617/1-Ant.tar
                                ${outputDir}/pl-617/2-Ant.zip
                                ${outputDir}/pl-617/3-Ant.zip
                                ${outputDir}/pl-617/4-Ant.zip
                                ${outputDir}/pl-617/5-Ant.tar.gz
                                ${outputDir}/pl-617/1-TrueZip.tar
                                ${outputDir}/pl-617/2-TrueZip.zip
                                ${outputDir}/pl-617/3-TrueZip.tar.gz
                            </assertFiles>
                            <assertGroovy>
                                <!-- Ant compression: tar, 3 compression levels (0, 5, 9), tar.gz -->
                                new File( outputDir, 'pl-617/1-Ant.tar' ).size() > new File( outputDir, 'pl-617/2-Ant.zip'    ).size()
                                new File( outputDir, 'pl-617/2-Ant.zip' ).size() > new File( outputDir, 'pl-617/3-Ant.zip'    ).size()
                                new File( outputDir, 'pl-617/3-Ant.zip' ).size() > new File( outputDir, 'pl-617/4-Ant.zip'    ).size()
                                new File( outputDir, 'pl-617/4-Ant.zip' ).size() > new File( outputDir, 'pl-617/5-Ant.tar.gz' ).size()

                                <!-- TrueZip compression: tar, zip, tar.gz. Better in all categories! -->
                                new File( outputDir, 'pl-617/1-Ant.tar'    ).size() > new File( outputDir, 'pl-617/1-TrueZip.tar'    ).size()
                                new File( outputDir, 'pl-617/4-Ant.zip'    ).size() > new File( outputDir, 'pl-617/2-TrueZip.zip'    ).size()
                                new File( outputDir, 'pl-617/5-Ant.tar.gz' ).size() > new File( outputDir, 'pl-617/3-TrueZip.tar.gz' ).size()
                            </assertGroovy>
                        </configuration>
                    </execution>
                    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                    <!-- http://evgeny-goldin.org/youtrack/issue/pl-550 -->
                    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                    <execution>
                        <id>http://evgeny-goldin.org/youtrack/issue/pl-550</id>
                        <goals>
                            <goal>assert</goal>
                        </goals>
                        <phase>test</phase>
                        <configuration>
                            <assertFiles>
                                ${outputDir}/pl-550
                                ${outputDir}/pl-550/dependency-plugin
                                ${outputDir}/pl-550/all
                                ${outputDir}/pl-550/single
                                ${outputDir}/pl-550/compile
                                ${outputDir}/pl-550/test
                                ${outputDir}/pl-550/provided
                                ${outputDir}/pl-550/optional-1
                                ${outputDir}/pl-550/optional-2

                                ${outputDir}/pl-550/1
                                ${outputDir}/pl-550/2
                                ${outputDir}/pl-550/3
                                ${outputDir}/pl-550/4
                                ${outputDir}/pl-550/5
                                ${outputDir}/pl-550/6
                                ${outputDir}/pl-550/7
                                ${outputDir}/pl-550/8
                                ${outputDir}/pl-550/9

                                ${outputDir}/pl-550/10
                                ${outputDir}/pl-550/12
                                ${outputDir}/pl-550/22
                                ${outputDir}/pl-550/32
                                ${outputDir}/pl-550/42
                                ${outputDir}/pl-550/52
                                ${outputDir}/pl-550/62
                                ${outputDir}/pl-550/72
                                ${outputDir}/pl-550/82
                                ${outputDir}/pl-550/92
                                ${outputDir}/pl-550/102
                            </assertFiles>
                            <assertGroovy>
                                <!--new File( outputDir, 'pl-550/1' ).listFiles().size() == ${springDependencies}-->
                                <!--new File( outputDir, 'pl-550/2' ).listFiles().size() == ${springDependencies}-->
                            </assertGroovy>
                            <assertEqual>
                                <!--${outputDir}/pl-550/1|${outputDir}/pl-550/2-->
                            </assertEqual>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>
</project>
