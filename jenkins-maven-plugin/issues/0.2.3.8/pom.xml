<?xml version="1.0" encoding="UTF-8"?>

<project xmlns              = "http://maven.apache.org/POM/4.0.0"
         xmlns:xsi          = "http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation = "http://maven.apache.org/POM/4.0.0
                               http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>
    <groupId>com.github.goldin.maven-plugins-test</groupId>
    <artifactId>jenkins-maven-plugin.issues-0.2.3.8</artifactId>
    <packaging>pom</packaging>
    <version>0.1-SNAPSHOT</version>
    <name>[${project.groupId}:${project.artifactId}:${project.version}]</name>


    <parent>
        <groupId>com.github.goldin.maven-plugins-test</groupId>
        <artifactId>jenkins-maven-plugin.issues</artifactId>
        <version>0.1-SNAPSHOT</version>
    </parent>


    <properties>
        <jobsToTest>3</jobsToTest>
    </properties>


    <build>
        <plugins>

            <plugin>
                <groupId>com.github.goldin</groupId>
                <artifactId>jenkins-maven-plugin</artifactId>
                <version>${plugins-version}</version>
                <executions>
                    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                    <!-- http://evgeny-goldin.org/youtrack/issue/pl-587 -->
                    <!-- "Job parameter ordering should be predictive"  -->
                    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
                    <execution>
                        <id>http://evgeny-goldin.org/youtrack/issue/pl-587</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>generate</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${configs-outputDirectory}</outputDirectory>
                            <generationPom>https://github.com/evgeny-goldin/maven-plugins-test/blob/master/jenkins-maven-plugin/issues/0.2.3.8/pom.xml</generationPom>
                            <jenkinsUrl>http://evgeny-goldin.org/jenkins</jenkinsUrl>
                            <timestamp>false</timestamp>
                            <jobs>
                                <job>
                                    <id>pl-587</id>
                                    <abstract>true</abstract>
                                    <scmType>svn</scmType>
                                    <jobType>maven</jobType>
                                    <jdkName>jdk1.6.0</jdkName>
                                    <mavenName>apache-maven-3</mavenName>
                                    <useUpdate>true</useUpdate>
                                    <doRevert>false</doRevert>
                                    <daysToKeep>10</daysToKeep>
                                    <numToKeep>10</numToKeep>
                                    <mavenGoals>-B -e clean install</mavenGoals>
                                    <buildOnSNAPSHOT>true</buildOnSNAPSHOT>
                                    <parameters>
                                        <parameter>
                                            <name>1</name>
                                            <type>string</type>
                                            <value>1</value>
                                        </parameter>
                                        <parameter>
                                            <name>2</name>
                                            <type>string</type>
                                            <value>2</value>
                                        </parameter>
                                        <parameter>
                                            <name>3</name>
                                            <type>choice</type>
                                            <value>13,23,33</value>
                                        </parameter>
                                    </parameters>
                                </job>
                                <job>
                                    <id>pl-587-1</id>
                                    <parent>pl-587</parent>
                                    <repository>
                                        <remote>http://google-guice.googlecode.com/svn/trunk</remote>
                                    </repository>
                                    <parameters>
                                        <parameter>
                                            <name>4</name>
                                            <type>choice</type>
                                            <value>14,24,34,44</value>
                                        </parameter>
                                        <parameter>
                                            <name>5</name>
                                            <type>string</type>
                                            <value>5</value>
                                        </parameter>
                                        <parameter>
                                            <name>6</name>
                                            <type>bool</type>
                                            <value>true</value>
                                        </parameter>
                                    </parameters>
                                </job>
                                <job>
                                    <id>pl-587-2</id>
                                    <parent>pl-587</parent>
                                    <repository>
                                        <remote>http://google-guice.googlecode.com/svn/trunk</remote>
                                    </repository>
                                    <parameters>
                                        <parameter>
                                            <name>2</name>
                                            <type>choice</type>
                                            <value>144,244,344,444</value>
                                        </parameter>
                                        <parameter>
                                            <name>3</name>
                                            <type>string</type>
                                            <value>5</value>
                                        </parameter>
                                        <parameter>
                                            <name>6</name>
                                            <type>bool</type>
                                            <value>true</value>
                                        </parameter>
                                    </parameters>
                                    <blockBuildWhenDownstreamBuilding>true</blockBuildWhenDownstreamBuilding>
                                    <blockBuildWhenUpstreamBuilding>true</blockBuildWhenUpstreamBuilding>
                                </job>
                                <job>
                                    <id>pl-595</id>
                                    <jobType>free</jobType>
                                    <disabled>true</disabled>
                                    <tasks>
                                        <ant>
                                            <antName>myAntName</antName>
                                            <targets>clean</targets>
                                            <buildFile>build file</buildFile>
                                        </ant>
                                    </tasks>
                                </job>
                            </jobs>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

</project>
